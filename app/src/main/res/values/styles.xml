<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>
    <style name="TileLeft">
        <item name="android:layout_width">120dp</item>
        <item name="android:layout_height">100dp</item>
        <item name="android:textColor">#ffffff</item>
        <item name="android:background">@color/colorTileLeft</item>
    </style>
    <style name="TileRight">
        <item name="android:layout_width">110dp</item>
        <item name="android:layout_height">100dp</item>
        <item name="android:textColor">#ffffff</item>
        <item name="android:background">@color/colorTileRight</item>
        <item name="android:ems">10</item>
    </style>
    <style name="Image">
        <item name="android:layout_width">125dp</item>
        <item name="android:layout_height">100dp</item>
    </style>


</resources>
